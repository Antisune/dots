            		var amazon_assoc_ir_f_call_acap = function(map) {
			var logTypeStr = "", foresterURL, json;
			if(typeof JSON !== 'undefined') json = JSON.stringify(map);
			else if(typeof amzn_assoc_utils !== 'undefined') json = amzn_assoc_utils.stringify(map);
			else return;
			if(typeof map.logType !== "undefined") logTypeStr = "&logType=" + map.logType;
			// forester URLs are of format //<end_point>/<api_version>/<channel_id>/<channel_version>/<OPERATION>/
			// Depending on operation, we either pass the data in the URI or we pass them as query parameters
			// if operation is OP, data must be in query parameters while if operation is TOP,
			// data must be in the URI itself
			foresterURL = "//fls-na.amazon-adsystem.com/1/acap/1/OE/";
						    foresterURL = foresterURL + encodeURIComponent(json) + "?cb=" + (new Date()).getTime() + "&logType=" + logTypeStr;
						(new Image()).src = foresterURL;
		};
                		var amazon_assoc_ir_f_call_associates_ads = function(map) {
			var logTypeStr = "", foresterURL, json;
			if(typeof JSON !== 'undefined') json = JSON.stringify(map);
			else if(typeof amzn_assoc_utils !== 'undefined') json = amzn_assoc_utils.stringify(map);
			else return;
			if(typeof map.logType !== "undefined") logTypeStr = "&logType=" + map.logType;
			// forester URLs are of format //<end_point>/<api_version>/<channel_id>/<channel_version>/<OPERATION>/
			// Depending on operation, we either pass the data in the URI or we pass them as query parameters
			// if operation is OP, data must be in query parameters while if operation is TOP,
			// data must be in the URI itself
			foresterURL = "//fls-na.amazon-adsystem.com/1/associates-ads/1/OP/r/json";
						    foresterURL = foresterURL + "?cb=" + (new Date()).getTime() + logTypeStr + "&p=" + encodeURIComponent(json);
						(new Image()).src = foresterURL;
		};
    var amazon_assoc_ir_f_call = amazon_assoc_ir_f_call_associates_ads;
               var amazon_assoc_aax_f_call = function(aax_map, pixelURL) {
       var json,payload;
       if(typeof pixelURL === 'undefined') return;
       if(typeof JSON !== 'undefined') json = JSON.stringify(aax_map);
       else if(typeof amzn_assoc_utils !== 'undefined') json = amzn_assoc_utils.stringify(aax_map);
       else return;
       payload = encodeURIComponent(json);
       if(typeof pixelURL === "string" && pixelURL != "") (new Image()).src = pixelURL + '?assoc_payload='+ payload;
   };
          
            window.runtimeManager = amzn_assoc_utils.assocRuntime["0"];
            window.backupRuntime = {"trackingId": "customcomputer0e",
                                    "url": ((window.location != window.parent.location)?document.referrer:document.location.href),
                                    "aaxRequestId": "" };
                                    
            var recordInteraction = function(data) {
                                    runtimeManager.retrieveRuntime(function(rt) {
                        data.tag = rt["trackingId"];
                        data.region = "US";
                        data.linkCode = "w24";
                        data.creative = "496521";
                        data.campaignId = "212361";
                        data.linkId = rt["linkid"];
                        data.placement = "KVVT73PKIGXN7OML";
                        data.refUrl = rt["url"] ||
                        ((window.location != window.parent.location) ?
                            document.referrer : document.location.href);
                        data.refUrl = data.refUrl.substring(0, 256);
                        if(typeof data.pixelURL === "undefined") {
                        	data.pixelURL = rt["pixelURL"];
                        }
                        if(typeof data.clickURL === "undefined") {
                        	data.clickURL = rt["clickURL"];
                        }
                
                        amazon_assoc_ir_f_call(data);
                        if(typeof rt["pixelURL"] !== "undefined") {
                       		amazon_assoc_aax_f_call(data,data.pixelURL);
                       	}
                    	
                    }, 1000, window.backupRuntime);

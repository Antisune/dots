                // go through all the links on the page and add parameter ref-refURL
    var addRefUrls = function(allLinks, linkid) {
        var refRefUrl = new RegExp("^http://.*(amazon|endless|myhabit|amazonwireless|javari|smallparts)\\.(com|ca|co\\.jp|de|fr|co\\.uk|cn|it|es)/.*tag=.*", "i"),
            pageUrl = (window.location != window.parent.location) ? document.referrer : document.location,
            href, results, newUrl, i,
            encodeStr = function(b) {
                return b && encodeURIComponent(b).replace(/&/g, "&amp;").replace(/"/g, "&quot;").replace(/</g, "&lt;").replace(/>/g, "&gt;");
            };
        for (i = 0; i < allLinks.length; i++) {
            allLinks[i].rel = "nofollow";
            href = new String(allLinks[i].href);
            if (results = href.match(refRefUrl)) {
                newUrl = href + "&ref-refURL=" + encodeStr(pageUrl);
                if(typeof linkid === "string" && linkid !== "") newUrl = newUrl + "&linkId=" + linkid;
                allLinks[i].href = newUrl;
            }
        }
    };
                // go through all the links on the page and wrap them behind AAX redirect click URL
    var addClickUrls = function(allLinks, clickPixel, impressionToken, slotNum) {
        if(typeof clickPixel === 'undefined') return;
        else {
            for(var i = 0; i < allLinks.length; i++) {
        	    var parser = document.createElement('a');
        	    parser.href = allLinks[i].href;
        	    if(parser.search) parser.search += "&imprToken=" + impressionToken + "&slotNum=" + slotNum;
        	    else parser.search = "imprToken=" + impressionToken + "&slotNum=" + slotNum;
        	    allLinks[i].href = clickPixel + parser.href;
            }
        }
    };
                

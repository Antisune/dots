            var recordAcapFeedback = function(data) {
                runtimeManager.retrieveRuntime(function(rt) {
                    data.type = "af";
                    data.url = rt["url"] ||
                    ((window.location != window.parent.location) ?
                        document.referrer : document.location.href);
                    data.url = data.url.substring(0, 256);
                    if (typeof data.request_id === "undefined") {
                        data.request_id = rt["aaxRequestId"];
                    }
                    if(typeof data.pixelURL === "undefined") {
                        data.pixelURL = rt["pixelURL"];
                    }
                    if(typeof data.clickURL === "undefined") {
                        data.clickURL = rt["clickURL"];
                     }
                    data.mkt = 'ATVPDKIKX0DER';
                    amazon_assoc_ir_f_call_acap(data);
                    }, 1000, window.backupRuntime);

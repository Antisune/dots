function degis(id, yeni) {
    var player = id;
    jwplayer(player).load({
        file:"yeni",
        image:"https://www.watchcartoononline.io/back.jpg"
    });
    myFunction(id);
    setTimeout(function(){ jwplayer(player).play(); }, 4000);
}
function myFunction(id) {
    var y = "hide-" + id;
    var x = document.getElementById(y);
    x.style.display = "none";
}

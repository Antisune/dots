}function msieversion(){var b=window.navigator.userAgent;
var a=b.indexOf("MSIE ");
return a>0||!!navigator.userAgent.match(/Trident.*rv\:11\./)
}function getUsageInCsv(d){var e="";
var b=d.length;
for(var c=0;
c<b;
c++){var a=d[c]===null?"":'="'+d[c]+'"';
if(c!==b-1){e+=a+","
}else{e+=a
}}return e
}function getDataRow(c,d,b){for(var f in c[d]){if(c[d].hasOwnProperty(f)){var e=c[d][f];
var a;
if(f==="usages"){a=getUsageInCsv(e)
}else{a=c[d][f]==null?"":'="'+e+'"'
}b+=a+","
}}return{columnCsv:e,arrValue:a,dataRow:b}
}function jsonToCsvConvertor(m,c,j){var e=typeof m!="object"?JSON.parse(m):m;
var l="";
if(j){var n="";
for(var g in e[0]){if(e[0].hasOwnProperty(g)){n+=g+","
}}n=n.slice(0,-1);
l+=n+"\r\n"
}for(var d=0;
d<e.length;
d++){var h="";

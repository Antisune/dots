function setCtaAutoPath(){invalidateCookie();
var i="";
var f=";path=/";
var a="expires=";
var h=$("#ctaProdPagePath");
if(h.length){i=h.val()
}var e="";
var c=$("#ctaMainProductId");
if(c.length){e=c.val()
}var g=window.location.protocol==="https:"?"; secure":"";
var d=new Date();
d.setTime(d.getTime()+(5*60*1000));
var b=a+d.toUTCString();
document.cookie="ctaAutoPagePath="+i+"; "+b+g+f;
document.cookie="ctaMainProductId="+e+"; "+b+g+";path=/"
}function getCookie(d){var b=d+"=";
var a=document.cookie.split(";");
for(var e=0;
e<a.length;
e++){var f=a[e];
while(f.charAt(0)===" "){f=f.substring(1)
}if(f.indexOf(b)===0){return f.substring(b.length,f.length)
}}return""
}function validateIsSameProductClicked(a,f){if(a.indexOf("/")>-1){var e=a.split("/");
var d=a;
var c=a.length;
for(var b=e.length;
b>0;
b--){d=a.substr(0,c);
if(f===d){return true
}c=c-(e[b-1].length+1)
}}else{if(a===f){return true
}}return false
}function iterateProductList(e,g,h,a){var f=g.split(",");
for(var b=0;
b<f.length;
b++){for(var d=0;
d<a.length;
d++){var c=h[d];
if(""!==c&&""!==f[b]&&c===f[b]){return"product_"+e
}}}return""
}function validateAllProducts(d,a){for(var c=0;
c<a.length;
c++){var e=a[c];

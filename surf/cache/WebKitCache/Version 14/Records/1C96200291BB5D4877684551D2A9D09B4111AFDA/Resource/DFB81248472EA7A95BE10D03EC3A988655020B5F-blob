L++){if(M[L].indexOf("/")===-1){return displayErrorMessages("EPM L1 product category pages are not allowed for IP&M Product Alert. Please remove any EPM L1 from product list.")
}}return true
};
var o=function(L){L.allowBlank=false;
if(null!=L&&L.length>0){var M=z(L);
if(!M){return false
}}else{return displayErrorMessages("Verify the values of the marked fields")
}return true
};
function y(N){var L=true;
var O=N.getField("./startTime").getValue();
var M=N.getField("./endTime").getValue();
if((O==null||O==="")&&M!==""){showErrorMessage("On time is required when off time is set.");
L=false
}if(L&&(M==null||M==="")&&O!==""){showErrorMessage("Off time is required when on time is set.");
L=false
}if(L&&(O>M)){showErrorMessage("Please provide a valid date/time range.");
L=false
}return L
}var m=function(L){var N=L.getField("./product").getValue();
var O=L.getField("./notificationType").getValue();
if(O==="IP&M Product Alert"){var M=o(N);
if(!M){return false
}}else{N.allowBlank=true
}return y(L)
};
function J(O){var L;
if(O[0].indexOf("/")>-1){L=O[0].substring(0,O[0].indexOf("/"))
}else{L=O[0]
}var N=false;
for(var M=0;
M<O.length;
M++){if(O[M].indexOf("/")>-1){N=true;
break
}}return{firstval:L,secAlert:N}
}var h=function(M){var N=M.getField("./productPage").getValue();
if(N!==null&&N.length>1){var L=J(N);
if(L.secAlert===true){return q(N,L.firstval)
}else{return g(N,L.firstval)
}}};
var t=function(M){var N=M.getField("./sfdcProductMap").getValue();
if(N!==null&&N.length>1){var L=J(N);
if(L.secAlert===true){return q(N,L.firstval)
}else{return g(N,L.firstval)
}}};
function q(O,M){for(var N=0;
N<O.length;
N++){var L;
if(O[N].indexOf("/")>-1){L=O[N].substring(0,O[N].indexOf("/"))
}else{L=O[N]
}if(M!==L){showErrorMessage("Invalid product selection. Selected product pages must have same parent (sibling product pages).");
return false
}}return true
}function g(O,L){for(var N=0;
N<O.length;
N++){var M;
if(O[N].indexOf("/")>-1){M=O[N].substring(0,O[N].indexOf("/"))
}else{M=O[N]
}if(L!==M){showErrorMessage("Multiple EPM L1 product category pages not allowed.");
return false
}}return true
}var w=function(){var N=CQ.Ext.getCmp("multifieldProductCategories");
var L=N.minLimit;
var M=N.findByType("pathfield").length;
if(M<L){showErrorMessage("Configure at least "+L+" product category.");
return false
}return true
};
return{driversAndDownloads:D,newDriversAndDownload:A,productSpecifications:a,productSpecificationsValidation:s,supportUtilityBarDataEntry:x,supportUtilityBar:l,loadWarranty:c,supportTilesValidation:i,checkDownloadsFlag:F,contactUsDataEntrySupport:r,contactUsDataEntryFacebook:p,contactUsDataEntryTwitter:G,contactUsDataEntryYouTube:j,contactUsDataEntryOnlineService:K,contactUsDataEntryEmail:e,contactUsDataEntryEmailPreSales:H,contactUsDataEntryChat:n,contactUsDataEntryPhone:E,notificationFieldDisplay:k,getProducts:m,getProductsPage:h,getSfdcProductMap:t,checkProductCategoryFields:w}
})();

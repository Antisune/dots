        function isRedirectComment() {
            var result = false;
            try {
                var cuurentRealm = l.Common.getCurrentRealm();
                var reg = new RegExp(/\/{1}qrhome|\/{1}PremierSupport|\/{1}PremierSupportQR|\/{1}servicecardQR|\/{1}premiumcareqrhome|\/{1}premierqrhome/gi);
                if (cuurentRealm === 2 && reg.test(window.location.pathname)) {
                    result = true;
                }
                return result;
            } catch (e) {
                return false;
            }
        }

        function threeSteps() {
            var result = false;
            try {
                var cuurentRealm = l.Common.getCurrentRealm();
                var reg = new RegExp(/\/threesteps/gi);
                if (cuurentRealm === 2 && reg.test(window.location.pathname)) {
                    result = true;
                }
                return result;
            } catch (e) {
                return false;
            }
        }
        if (isRedirectComment()) {
            OOo.oo_tab = new OOo.Ocode({
                tab: {
                    position: 'right',
                    title: tabText,
                    tabType: 0,
                    verbiage: tabText,
                    iconPath: assetDirectory
                },
                disableShow: false
            });
        } else if (threeSteps()) {
            OOo.oo_tab = new OOo.Ocode({
                tab: {
                    position: 'right',
                    title: tabText,
                    tabType: 0,
                    verbiage: tabText,
                    iconPath: assetDirectory
                },
                referrerRewrite: [pageurl, (/\?/.test(w.location.href)) ? '&' : '?', 'olSegment=wpWebsite'].join(''),
                disableShow: false
            });
        } else {
            OOo.oo_tab = new OOo.Ocode({
                tab: {
                    position: 'right',
                    title: tabText,
                    tabType: 0,
                    verbiage: tabText,
                    iconPath: assetDirectory
                },
                disableShow: true
            });
            OOo.appendWaypoint('oo_tab');
        }

        OOo.oo_invite = new OOo.Invitation({

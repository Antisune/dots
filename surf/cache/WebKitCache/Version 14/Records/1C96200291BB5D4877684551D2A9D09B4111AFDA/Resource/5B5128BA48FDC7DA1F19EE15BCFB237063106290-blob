function g(i) { document.location.href = i; }

function openopinionlab(url) {
    window.open(url);
}

function isIE() { //ie?
    if (!!window.ActiveXObject || "ActiveXObject" in window) {
        return true;
    }
    else if (isNormalIE()) {
        return true;
    }
    else {
        return false;
    }
}

function isNormalIE() {
    if (window.navigator.userAgent.toLowerCase().indexOf("msie") >= 0)
        return true;
    else
        return false;
}

function isFirefox() {
    if (window.navigator.userAgent.toLowerCase().indexOf("firefox") >= 0)
        return true;
    else
        return false;
}

function isChrome() {
    if (window.navigator.userAgent.toLowerCase().indexOf("chrome") >= 0)
        return true;
    else
        return false;
}

function GetBrowserName() {
    var result = 'Other';
    var isie = isIE();
    var isfirefox = isFirefox();
    var ischrome = isChrome();
    if (isie) {
        result = "IE";
    }
    else if (isfirefox) {
        result = "Firefox";
    }
    else if (ischrome) {
        result = "Chrome";
    }
    return result;
}

function getDeviceType(){
    var result = "desktop";
    if (window.innerWidth > 991) {
        result = "desktop";
    }
    else if (window.innerWidth < 768) {
        result = "mobile";
    } else {
        result = "tablet";
    }
    return result;
}

function addLoadEvent(func) {
    var oldonload = window.onload;
    if (typeof window.onload != 'function') {
        window.onload = func;
    } else {
        window.onload = function () {
            oldonload();
            func();
        }
    }
}


addLoadEvent(loadOOoCustomVariable);

function loadOOoCustomVariable() 
{
    if (OOo.oo_invite) {
        OOo.oo_invite.options.customVariablesAppended.sAccount = (typeof s_account !== 'undefined' ? s_account : '');
        OOo.oo_invite.options.customVariablesAppended.pageName = (typeof s !== 'undefined' ? (typeof s.pageName !== 'undefined' ? s.pageName : '') : '');
        OOo.oo_invite.options.customVariablesAppended.microsite = (typeof s !== 'undefined' ? (typeof s.server !== 'undefined' ? s.server : '') : '');
        OOo.oo_invite.options.customVariablesAppended.visitorID = (typeof s !== 'undefined' ? (typeof s.prop22 !== 'undefined' ? s.prop22 : '') : '');
    }

    if (OOo.oo_waypoint) {
        OOo.oo_waypoint.options.categories.website.oCode.customVariables.sAccount = (typeof s_account !== 'undefined' ? s_account : '');
        OOo.oo_waypoint.options.categories.website.oCode.customVariables.pageName = (typeof s !== 'undefined' ? (typeof s.pageName !== 'undefined' ? s.pageName : '') : '');
        OOo.oo_waypoint.options.categories.website.oCode.customVariables.microsite = (typeof s !== 'undefined' ? (typeof s.server !== 'undefined' ? s.server : '') : '');
        OOo.oo_waypoint.options.categories.website.oCode.customVariables.visitorID = (typeof s !== 'undefined' ? (typeof s.prop22 !== 'undefined' ? s.prop22 : '') : '');

        OOo.oo_waypoint.options.categories.product.oCode.customVariables.sAccount = (typeof s_account !== 'undefined' ? s_account : '');
        OOo.oo_waypoint.options.categories.product.oCode.customVariables.pageName = (typeof s !== 'undefined' ? (typeof s.pageName !== 'undefined' ? s.pageName : '') : '');
        OOo.oo_waypoint.options.categories.product.oCode.customVariables.microsite = (typeof s !== 'undefined' ? (typeof s.server !== 'undefined' ? s.server : '') : '');
        OOo.oo_waypoint.options.categories.product.oCode.customVariables.visitorID = (typeof s !== 'undefined' ? (typeof s.prop22 !== 'undefined' ? s.prop22 : '') : '');

        OOo.oo_waypoint.options.categories.service.oCode.customVariables.sAccount = (typeof s_account !== 'undefined' ? s_account : '');
        OOo.oo_waypoint.options.categories.service.oCode.customVariables.pageName = (typeof s !== 'undefined' ? (typeof s.pageName !== 'undefined' ? s.pageName : '') : '');
        OOo.oo_waypoint.options.categories.service.oCode.customVariables.microsite = (typeof s !== 'undefined' ? (typeof s.server !== 'undefined' ? s.server : '') : '');
        OOo.oo_waypoint.options.categories.service.oCode.customVariables.visitorID = (typeof s !== 'undefined' ? (typeof s.prop22 !== 'undefined' ? s.prop22 : '') : '');
    }
}